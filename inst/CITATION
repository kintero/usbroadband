citHeader("To cite package 'usbroadband' in publications use:")
     
 year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", meta$Date, perl = TRUE)
 vers <- paste("R package version", meta$Version)
 
 citEntry(entry="Manual",
		  title = "usbroadband: Data for the U.S. National Broadband Map",
		  author = personList(as.person("Thomas J. Leeper")),
		  year = year,
		  note = vers,
		  textVersion =
		  paste("Thomas J. Leeper (",
				year,
				"). usbroadband: U.S. Data for the National Broadband Map. ",
				vers, ".", sep=""))